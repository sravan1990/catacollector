pipeline {
    agent any

    stages {
        stage('Code Checkout') {
            steps {
                echo 'Checkingout code..'
            }
        }
        stage('Build') {
            steps {
                echo 'building..'
            }
        }
        stage('Execute script') {
            steps {
                echo 'Executing shell scripts....'
            }
        }
    }
}